class Solution:
    def judgeCircle(self, moves):
        x,y=0,0
        for i in moves:
            if i=="R":
                x=x+1
            elif i=="L":
                x=x-1
            elif i=="U":
                y=y+1
            elif i=="D":
                y=y-1
        return x==0 and y==0
            
